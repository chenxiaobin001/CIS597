    2px;\">\n\t\t\t\t\t\t\t\tadjective:\n\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t<span class=\"category_data\"
    style=\"font-weight:normal; vertical-align:top;\">Brazilian </span></div>\n\t\t\t\t\t\t\t\n\t\t\t\t\t</td>\n\t\t\t\t</tr>\n\t\t\t\t<tr>\n\t\t\t\t\t<td
